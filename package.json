{
  "name": "@rhysforyou/gatsby-plugin-safari-site-icon",
  "version": "1.0.1",
  "description": "Add an SVG icon for Safari to your Gatsby site",
  "main": "index.js",
  "homepage": "https://github.com/rhysforyou/gatsby-plugin-safari-site-icon",
  "repository": "https://github.com/rhysforyou/gatsby-plugin-safari-site-icon.git",
  "author": "Rhys Powell <rhys@rpowell.me>",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "icon",
    "safari"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "clean": "rm {gatsby-node,gatsby-ssr,common,svgo}.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.4",
    "cross-env": "^5.1.4",
    "gatsby": "^2.1.31"
  },
  "peerDependencies": {
    "gatsby": "2.x"
  },
  "dependencies": {
    "svgo": "^1.2.0"
  }
}
